#!/usr/bin/env phasm

std = Import("gh:aliclark/phasm-scratch/master/std.py")

abs_asm   = Import("gh:aliclark/phasm-scratch/master/asm-x64.psm")
abs_elf   = Import("gh:aliclark/phasm-scratch/master/elf.psm")
abs_proc  = Import("gh:aliclark/phasm-scratch/master/proc.psm")
abs_sys   = Import("gh:aliclark/phasm-scratch/master/syscall-linux-x64.psm")
abs_util  = Import("gh:aliclark/phasm-scratch/master/util.psm")
abs_hello = Import("gh:aliclark/phasm-scratch/master/hello.psm")

asm  = abs_asm(std)
elf  = abs_elf(std)
proc = abs_proc(asm)
sys  = abs_sys(std, asm)
util = abs_util(std, asm, sys)

abs_hello(std, asm, elf, proc, sys, util)
