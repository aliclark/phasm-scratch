#!/usr/bin/env phasm

std = Import("gh:aliclark/phasm-scratch/0.2/std.py")

abs_asm   = Import("gh:aliclark/phasm-scratch/0.2/asm-x64.psm")
abs_elf   = Import("gh:aliclark/phasm-scratch/0.2/elf.psm")
abs_proc  = Import("gh:aliclark/phasm-scratch/0.2/proc.psm")
abs_sys   = Import("gh:aliclark/phasm-scratch/0.2/syscall-linux-x64.psm")
abs_util  = Import("gh:aliclark/phasm-scratch/0.2/util.psm")
abs_hello = Import("gh:aliclark/phasm-scratch/0.2/hello.psm")

asm  = abs_asm(std)
elf  = abs_elf(std)
proc = abs_proc(asm)
sys  = abs_sys(std, asm)
util = abs_util(std, asm, sys)

abs_hello(std, asm, elf, proc, sys, util)
